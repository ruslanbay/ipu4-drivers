From a6d9343bd07625ff09e4ea99f864dd51c165e4e2 Mon Sep 17 00:00:00 2001
From: ruslanbay <67730802+ruslanbay@users.noreply.github.com>
Date: Sat, 27 Dec 2025 15:45:22 +0100
Subject: [PATCH 34/41] rename sensor_async_sd to sensor_async_subdev

---
 drivers/media/pci/intel/ipu-isys.c | 8 ++++----
 drivers/media/pci/intel/ipu-isys.h | 2 +-
 2 files changed, 5 insertions(+), 5 deletions(-)

diff --git a/drivers/media/pci/intel/ipu-isys.c b/drivers/media/pci/intel/ipu-isys.c
index f2adee34a..e09c0cde3 100644
--- a/drivers/media/pci/intel/ipu-isys.c
+++ b/drivers/media/pci/intel/ipu-isys.c
@@ -402,8 +402,8 @@ static int isys_notifier_bound(struct v4l2_async_notifier *notifier,
 {
 	struct ipu_isys *isys =
 		container_of(notifier, struct ipu_isys, notifier);
-	struct sensor_async_sd *s_asd =
-		container_of(asd, struct sensor_async_sd, asd);
+	struct sensor_async_subdev *s_asd =
+		container_of(asd, struct sensor_async_subdev, asd);
 	int ret;
 
 	if (s_asd->csi2.port >= isys->pdata->ipdata->csi2.nports) {
@@ -454,7 +454,7 @@ static int isys_notifier_init(struct ipu_isys *isys)
 		struct v4l2_fwnode_endpoint vep = {
 			.bus_type = V4L2_MBUS_CSI2_DPHY
 		};
-		struct sensor_async_sd *s_asd;
+		struct sensor_async_subdev *s_asd;
 		struct fwnode_handle *ep;
 
 		ep = fwnode_graph_get_endpoint_by_id(dev_fwnode(dev), i, 0,
@@ -469,7 +469,7 @@ static int isys_notifier_init(struct ipu_isys *isys)
 		}
 
 		s_asd = v4l2_async_nf_add_fwnode_remote(&isys->notifier, ep,
-							struct sensor_async_sd);
+							struct sensor_async_subdev);
 		if (IS_ERR(s_asd)) {
 			ret = PTR_ERR(s_asd);
 			dev_err(dev, "add remove fwnode failed: %d\n", ret);
diff --git a/drivers/media/pci/intel/ipu-isys.h b/drivers/media/pci/intel/ipu-isys.h
index 606c0ebfc..f0321a797 100644
--- a/drivers/media/pci/intel/ipu-isys.h
+++ b/drivers/media/pci/intel/ipu-isys.h
@@ -64,7 +64,7 @@ struct ipu_isys_csi2_config {
 	unsigned int port;
 };
 
-struct sensor_async_sd {
+struct sensor_async_subdev {
 	struct v4l2_async_subdev asd;
 	struct ipu_isys_csi2_config csi2;
 };
-- 
